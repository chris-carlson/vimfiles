extends html

# Maven

snippet dep "dependency declaration"
<dependency>
	<groupId>${1:group}</groupId>
	<artifactId>${2:artifact}</artifactId>
	<version>${3:version}</version>
</dependency>
endsnippet

snippet comp "compiler settings"
<properties>
	<maven.compiler.source>${1:version}</maven.compiler.source>
	<maven.compiler.target>$1</maven.compiler.target>
</properties>
endsnippet
